{
    "name": "dinkbit/payme",
    "description": "Multi-gateway payment processing library for Laravel",
    "license": "MIT",
    "keywords": [
        "payme",
        "payments",
        "laravel",
        "dinkbit"
    ],
    "authors": [
        {
            "name": "dinkbit",
            "homepage": "https://github.com/dinkbit/payme"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~4.1|~5.0",
        "guzzlehttp/guzzle": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Dinkbit\\Payme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dinkbit\\Payme\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
